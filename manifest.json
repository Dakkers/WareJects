{
 "name": "WareJects",
 "version": "0.1",
 "description": "Keep track of your software and hardware ideas and projects.",

 "permissions": [ "tabs", "https://*/*", "http://*/*"],

 "content_scripts": [
  {
  "matches": ["https://*/*"],
  "js" : ["jquery.min.js", "opentab.js"]
  }
 ],

 "manifest_version": 2,
 "minimum_chrome_version": "17",

 "browser_action": {
  "default_icon": "icon.png",
  "default_popup": "popup.html"
 }
}